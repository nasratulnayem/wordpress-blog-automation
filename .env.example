WP_BASE_URL=https://your-site.com
WP_USERNAME=your-wp-username
WP_APP_PASSWORD=your-wp-application-password

# Gemini
GEMINI_API_KEY=your-gemini-api-key
GEMINI_API_KEYS=
GEMINI_MODEL=gemini-1.5-flash-latest
GEMINI_SDK=auto

# Prompts
CUSTOM_PROMPT=

# Blog post SEO meta keys (WP posts)
META_TITLE_KEY=yoast_wpseo_title
META_DESCRIPTION_KEY=yoast_wpseo_metadesc
USE_EXCERPT_FOR_META_DESCRIPTION=true

# WooCommerce product Yoast meta keys
PRODUCT_META_TITLE_KEY=_yoast_wpseo_title
PRODUCT_META_DESCRIPTION_KEY=_yoast_wpseo_metadesc
PRODUCT_FOCUS_KEYWORD_KEY=_yoast_wpseo_focuskw

FLASK_SECRET_KEY=change-me

